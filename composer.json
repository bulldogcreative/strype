{
    "name": "bulldog/strype",
    "description": "Wrapper for Stripe",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Durfee",
            "email": "levi.durfee@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bulldog\\Strype\\": "src/"
        }
    },
    "require": {
        "stripe/stripe-php": "^6.19",
        "php": "^7.1",
        "bulldog/id": "^1.0"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^2.5",
        "phpunit/phpunit": "^7.4",
        "symfony/var-dumper": "^4.1",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "scripts": {
        "fix": "php-cs-fixer fix ./src --rules=@Symfony && php-cs-fixer fix ./tests --rules=@Symfony"
    }
}
